@model List<string>

    <h1 style="color:cadetblue">MUNICIPIOS</h1>

    <a asp-controller="Catastro" asp-action="Index" >Volver a Index</a>
    <ul class="list-group">
       @foreach (string mun in Model)
    {
         <li class="list-group-item list-group-item-warning" style="color:coral">
             @mun
        </li>
    }
    </ul>